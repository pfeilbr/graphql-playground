{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": [
        false
    ],
    "jsx-no-lambda": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "no-any": false,
    "no-unsafe-any": false,
    "no-shadowed-variable": false

  }  
}
