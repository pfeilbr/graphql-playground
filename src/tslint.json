{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [
            false
        ],
        "jsx-no-lambda": [
            false
        ],
        "no-unused-expression": [
            false
        ],
        "no-empty": [
            false
        ],
        "semicolon": [true, "never"],
        "object-literal-sort-keys": false,
        "object-literal-quote-keys": false
    },
    "rulesDirectory": []
}